<metal:main metal:use-macro="load: master.pt">
    <div metal:fill-slot="content">
        <div class="panel panel-default" tal:repeat="row rows">
            <div class="panel-body">
                <b>UUID:</b><br>${row[0]}<br><br>
                <b>ID:</b><br>${row[1]}<br><br>
                <b>Date:</b><br>${row[2].isoformat(' ')}<br><br>
                <b>Data:</b><br>${str(row[3:])}<br><br>
            </div>
        </div>

    </div>

</metal:main>