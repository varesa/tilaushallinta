<metal:main metal:use-macro="load: tilaushallinta.templates:/master.pt">
    <metal:main metal:fill-slot="title">Tilauslista</metal:main>

    <metal:main metal:fill-slot="head_custom">
        <link rel="stylesheet" type="text/css" href="/static/css/order_list.css">
    </metal:main>

    <metal:main metal:fill-slot="content">
        <div class="panel panel-default">
            <div class="panel-heading">
                Avoinna olevat tilaukset
            </div>
            <metal:main metal:use-macro="load: tilaushallinta.templates:/snippets/orders_table.pt" tal:define="tilaukset tilaukset_auki"></metal:main>
        </div>
        <br>
        <div class="panel panel-default">
            <div class="panel-heading">
                Valmiit tilaukset
            </div>
            <metal:main metal:use-macro="load: tilaushallinta.templates:/snippets/orders_table.pt" tal:define="tilaukset tilaukset_valmiit"></metal:main>
        </div>

    </metal:main>
</metal:main>