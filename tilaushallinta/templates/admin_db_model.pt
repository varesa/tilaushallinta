<metal:main metal:use-macro="load: admin.pt">
    <div metal:fill-slot="admin-content">
        <div class="panel panel-default">
            <table class="table table-hover">
                <tr>
                    <th>&nbsp;</th>
                    <th>UUID&nbsp;&nbsp;&nbsp;&nbsp;</th>
                    <th>ID&nbsp;&nbsp;&nbsp;&nbsp;</th>
                    <th>Date&nbsp;&nbsp;&nbsp;&nbsp;</th>
                    <th>Data&nbsp;&nbsp;&nbsp;&nbsp;</th>

                </tr>
                <tr tal:repeat="row rows" onclick='window.location.href="/admin/db/${model}/${row[1]}"'>
                    <td><input type="checkbox" name="${row[0]}">&nbsp;&nbsp;&nbsp;&nbsp;</td>
                    <td>${row[0]}&nbsp;&nbsp;&nbsp;&nbsp;</td>
                    <td>${row[1]}&nbsp;&nbsp;&nbsp;&nbsp;</td>
                    <td><?python if row[2]: row[2].isoformat(' ') ?>&nbsp;&nbsp;&nbsp;&nbsp;</td>
                    <td>${str(row[3:])}&nbsp;&nbsp;&nbsp;&nbsp;</td>
                </tr>
            </table>
        </div>
    </div>

</metal:main>